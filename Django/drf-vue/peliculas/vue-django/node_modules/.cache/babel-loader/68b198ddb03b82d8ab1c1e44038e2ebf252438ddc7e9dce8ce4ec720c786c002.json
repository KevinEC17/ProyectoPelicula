{"ast":null,"code":"import axios from 'axios';\nimport NavigationComponentVue from '../components/NavigationComponent.vue';\nexport default {\n  components: {\n    NavigationComponentVue\n  },\n  name: 'HomeView',\n  data() {\n    return {\n      allFilms: {},\n      filteredFilms: [],\n      categoryRecived: null\n    };\n  },\n  methods: {\n    categoryID(categoryID, categoryName) {\n      this.categoryRecived = categoryName;\n      if (categoryID) {\n        this.filteredFilms = this.allFilms.filter(film => film.category === categoryID);\n      } else {\n        this.filteredFilms = this.allFilms;\n      }\n    },\n    resetFilter() {\n      this.categoryRecived = null;\n      this.filteredFilms = this.allFilms;\n    }\n  },\n  mounted() {\n    axios.get('http://127.0.0.1:8000/api/films/').then(response => {\n      this.allFilms = response.data;\n      this.filteredFilms = this.allFilms;\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"names":["axios","NavigationComponentVue","components","name","data","allFilms","filteredFilms","categoryRecived","methods","categoryID","categoryName","filter","film","category","resetFilter","mounted","get","then","response","catch","error","console","log"],"sources":["C:\\Users\\Usuario\\Desktop\\Django\\drf-vue\\peliculas\\vue-django\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n\n  <navigation-component-vue @getCategoryID=\"categoryID\"></navigation-component-vue>\n  <div v-if=\"categoryRecived\">\n    <h3>Peliculas de {{  categoryRecived }}</h3>\n    <button class=\"btn btn-warning\" @click=\"resetFilter\">Mostrar todas las peliculas</button>\n  </div>\n\n  <div>\n    <div class=\"d-flex flex-wrap\">\n      <div class=\"card m-3\" style=\"width: 15rem;\" v-for=\"film in filteredFilms\" :key=\"film.id\">\n        <img :src=\"film.image\" id=\"imgPel\" class=\"img-fluid p-3\" alt=\"Poster de {{ film.title }}\" style=\"width: 15rem; height: 21rem\">\n        <div class=\"card-body\">\n          <h3 class=\"card-title mb-3\">{{ film.title }}</h3>\n          <p class=\"card-text mb-1\">{{ film.description }}</p>\n          <p class=\"card-text mb-1\">{{ film.publication }}</p>\n          <p class=\"card-text mb-1\">{{ film.category_name }}</p>\n        </div>\n      </div>\n    </div>\n    <hr>\n  </div>\n</template>\n\n<script>\n  import axios from 'axios'\n  import NavigationComponentVue from '../components/NavigationComponent.vue'\n\n  export default {\n    components: {\n      NavigationComponentVue\n    },\n    name: 'HomeView',\n    data() {\n      return {\n        allFilms: {},\n        filteredFilms: [],\n        categoryRecived: null,\n      }\n    },\n\n    methods: {\n      categoryID(categoryID, categoryName) {\n        this.categoryRecived = categoryName\n        if(categoryID){\n          this.filteredFilms = this.allFilms.filter((film) => film.category === categoryID)\n        }\n        else{\n          this.filteredFilms = this.allFilms\n        }\n      },\n      resetFilter(){\n        this.categoryRecived = null\n        this.filteredFilms = this.allFilms\n      },\n    },\n    \n    mounted() {\n      axios.get('http://127.0.0.1:8000/api/films/')\n      .then( response => {\n        this.allFilms = response.data\n        this.filteredFilms = this.allFilms\n      })\n      .catch( error => {\n        console.log(error)\n      })\n    }\n  }\n</script>\n\n<style>\n\n</style>"],"mappings":"AAyBE,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,sBAAqB,MAAO,uCAAsC;AAEzE,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CAAC,CAAC;MACZC,aAAa,EAAE,EAAE;MACjBC,eAAe,EAAE;IACnB;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,UAAUA,CAACA,UAAU,EAAEC,YAAY,EAAE;MACnC,IAAI,CAACH,eAAc,GAAIG,YAAW;MAClC,IAAGD,UAAU,EAAC;QACZ,IAAI,CAACH,aAAY,GAAI,IAAI,CAACD,QAAQ,CAACM,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,QAAO,KAAMJ,UAAU;MAClF,OACI;QACF,IAAI,CAACH,aAAY,GAAI,IAAI,CAACD,QAAO;MACnC;IACF,CAAC;IACDS,WAAWA,CAAA,EAAE;MACX,IA