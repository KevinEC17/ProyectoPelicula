methods: {\r\n            isValidForm(){\r\n                let valid = true;\r\n                if(!this.username){\r\n                    this.errors.username = \"Este campo no puede estar en blanco\";\r\n                }\r\n                else{\r\n                    this.errors.username = \"\";\r\n                }\r\n                if(!this.password){\r\n                    this.errors.password = \"Este campo no puede estar en blanco\";\r\n                }\r\n                else{\r\n                    this.errors.password = \"\";\r\n                }\r\n                if(this.errors.username || this.errors.password ){\r\n                    valid = false;\r\n                }\r\n                return valid;\r\n            },\r\n            submitForm(){\r\n                if(this.isValidForm()){\r\n                    axios.post('http://127.0.0.1:8000/login/',{username: this.username, pasword: this.password})\r\n                    .then(response => {\r\n                        console.log(response.data);\r\n                    })\r\n                    .catch(error => {\r\n                        /*if(error.reponse.data.non_field_errors) {\r\n                            this.errors.wrong_credentials = error.response.data.non_field_errors.join('');\r\n                        }*/\r\n                        console.log(error);\r\n                    })\r\n                }\r\n                else {\r\n                    console.log('Form no valid');\r\n                }\r\n            },\r\n        }\r\n    }\r\n</script>"],"mappings":"AA0BI,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAc;EACVC,IAAI,EAAE,OAAO;EACbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;QACJF,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAC,EAAE;QACXE,iBAAiB,EAAE;MACvB;IACJ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAE;MACT,IAAIC,KAAI,GAAI,IAAI;MAChB,IAAG,CAAC,IAAI,CAACN,QAAQ,EAAC;QACd,IAAI,CAACE,MAAM,CAACF,QAAO,GAAI,qCAAqC;MAChE,OACI;QACA,IAAI,CAACE,MAAM,CAACF,QAAO,GAAI,EAA